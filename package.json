{
  "name": "webdriverio-typescript-primarybid",
  "version": "1.0.0",
  "description": "This is a test framework that is testing the website of Primary Bid  It is based on typescript, cucumberJS, webdriverIO and chai",
  "scripts": {
    "test": "./node_modules/.bin/wdio wdio.conf.js",
    "cucumber": "./node_modules/.bin/cucumber-js 'features/*.feature' --require 'step-definitions/**/*.ts'  --require 'support/**/*.ts'  --require 'page-objects/**/*.ts' --require-module ts-node/register --format node_modules/cucumber-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noorshah/webdriverio-typescript-primarybid.git"
  },
  "author": "noor us sabah shah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noorshah/webdriverio-typescript-primarybid/issues"
  },
  "homepage": "https://github.com/noorshah/webdriverio-typescript-primarybid#readme",
  "devDependencies": {
    "@types/cucumber": "^6.0.1",
    "@wdio/cli": "^6.3.0",
    "@wdio/cucumber-framework": "^6.3.0",
    "@wdio/local-runner": "^6.3.0",
    "@wdio/spec-reporter": "^6.3.0",
    "@wdio/sync": "^6.3.0",
    "chromedriver": "^83.0.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "wdio-chromedriver-service": "^6.0.3",
    "wdio-cucumberjs-json-reporter": "^2.0.2"
  }
}
